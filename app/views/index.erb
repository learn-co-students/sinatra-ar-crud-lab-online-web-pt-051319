<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Articles</title>
  </head>
  <body>
      <h1>Articles</h1>
      <% @articles.each do |article| %>
      <div>
        <h2><a href="articles/<%= article.id %>" title="<%= article.title %>"><%= article.title %></a></h2>
        <div><%= article.content %></div>
      </div>
      <% end %>

  </body>
</html>
